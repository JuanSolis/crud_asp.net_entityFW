@model IEnumerable<Loginasp.Models.User>

@{
    ViewBag.Title = "View";
}


<h2 class="text-center">Listado de Usuarios</h2>


<table class="table table-striped table-dark">
    <tr>
        <th>
            #
        </th>
        <th>
            @Html.DisplayNameFor(model => model.correo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.nombre_completo)
        </th>

    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.id)
        </td>
        <td>
            <a href="/Usuarios/Perfil/@item.id" class="nav-link">@Html.DisplayFor(modelItem => item.correo)</a>
        </td>

        <td>
            @Html.DisplayFor(modelItem => item.nombre_completo)
        </td>
        <td>
    </tr>
}

</table>
